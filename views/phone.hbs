<h1>This is {{title}}</h1>

<div class="container">
  <div class="row">
    <div class="col-5">
      <div class="card p-page">
        <div class="card-image">
          <img src="{{img}}" alt="jpg" />
          <span class="card-title">{{title}}</span>
        </div>
        <div class="card-content">
          <p>{{price}}$</p>
        </div>
        <div class="card-action">
          <a href="/delete/{{id}}">Delate</a>
        </div>
      </div>
    </div>
  </div>
</div>